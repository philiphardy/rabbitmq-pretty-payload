{
  "name": "RabbitMQ Pretty Payload",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
   "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon48.png",
    "default_popup": "browser-action/popup.html"
  },
  "description": "Make JSON payloads pretty",
  "version": "1.1",
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "*://*/"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "manifest_version": 2
}
