{
  "name": "RabbitMQ Pretty Payload",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
   "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon48.png"
  },
  "description": "Make JSON payloads pretty",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["http://localhost:15672/*"],
      "js": ["script.js"]
    }
  ],
  "manifest_version": 2
}
